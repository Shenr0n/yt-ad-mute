{
    "manifest_version": 3,
    "name": "YouTube Ad Muter",
    "version": "1.0.1",
    "description": "Mutes audio during YouTube ads",
    "permissions": [
      "tabs",
      "activeTab",
      "storage"
    ],
    "host_permissions": [
      "*://www.youtube.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["admute.js"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  